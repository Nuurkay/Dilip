        <field name="name">Membership PLan Sequence</field>
        <field name="code">membership.membership_line</field>
        <field name="number_increment">1</field>
        <field name="padding">4</field>
        <field name="prefix">MEPLAN/%(year)s/</field>
    </record>

</odoo>
